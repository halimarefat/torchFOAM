FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include "../meshSetup";

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform ($Ug $Vg 0); 

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"
    
    outlet
    {
        type            pressureInletOutletVelocity;
        value           $internalField;
    }
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }
}


